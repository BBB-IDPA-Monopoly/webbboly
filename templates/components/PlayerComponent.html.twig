<div id="player{{ player.id }}" class="card {% if isMe %}border-info border-3{% endif %} {{ this.getPlayerClass }}">
    <div class="card-body">
        <div class="card-title">
            <h6>{{ player.nickname }} {% if isMe %}<span class="badge bg-info">Me</span>{% endif %}</h6>
        </div>
        <p>{{ player.money }}</p>
    </div>
</div>
