<img src="https://placehold.co/50x50.png" class="img-fluid img-thumbnail" alt="{{ field.actionField.name }}">

{% if field.owner is null %}
    <span>{{ price }}$</span>
{% else %}
    <span class="text-{{ this.getPlayerClass(field.owner) }}"
          style="{% if field.mortgaged %}text-decoration: line-through;{% endif %}">
        {{ field.owner.nickname }} (w*{{ this.getUtilityRent() }})
    </span>
{% endif %}
